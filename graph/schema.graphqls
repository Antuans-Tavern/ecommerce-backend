# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  login(email: String!, password: String!): Login!
  register(data: Register!): Login!
}

type Login {
  token: String!
  user: User!
}

type User {
  id: ID!
  email: String!
  type: Int!
  profile: Profile
}

type Profile {
  name: String!
  lastname: String!
}

input Register {
  email: String!
  password: String!
  name: String!
  lastname: String!
}
